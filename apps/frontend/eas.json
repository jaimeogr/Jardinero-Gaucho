{
  "cli": {
    "version": "15.0.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "environment": "development",
      "developmentClient": true,
      "distribution": "internal",
      "node": "20.17.0",
      "cache": {
        "disabled": false
      }
    },
    "preview": {
      "environment": "preview",
      "distribution": "internal",
      "node": "20.17.0"
    },
    "production": {
      "environment": "production",
      "distribution": "store",
      "node": "20.17.0",
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
